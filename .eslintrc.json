{
  "extends": [
    "next/core-web-vitals",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "plugins": [
    "sort-keys-fix",
    "import"
  ],
  "rules": {
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "quotes": [
      "error",
      "backtick"
    ],
    "semi": [
      "warn",
      "never"
    ],
    "sort-keys-fix/sort-keys-fix": "warn"
  }
}